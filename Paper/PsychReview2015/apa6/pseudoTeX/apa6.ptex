  {\@textcite}%
  {\@textcite[]}%
}
\def\@textcite[#1]{%
  \@ifnextchar[%
  {\@@textcite[#1]}%
  {\@@textcite[#1][]}%
}
\def\@@textcite[#1][#2]#3{%
  \makeatletter%
  {\{ , , \makeCommaIntoSemicolon{#3}@{#2}@author-year\}}%
  \makeatother%
}
\let\Textcite=\textcite

