\newcommand\@secdef@nonum[7]{%
        \begingroup%
        \entity@paragraph@style=#1%
        \entity@paragraph@span@font=0%
        \entity@paragraph@just=#2%
        \entity@paragraph@spacebefore=#3%
        \entity@paragraph@spaceafter=#4%
        \entity@paragraph@keepwithnext=1%
